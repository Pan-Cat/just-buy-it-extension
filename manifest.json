{
   "manifest_version": 3,
   "minimum_chrome_version": "88",
   "name": "Just Buy It",
   "description": "就讓美好的購物揭開你生活的新頁",
   "version": "1.0.8",
   "homepage_url": "https://just-buy.it",
   "icons": {
      "128": "assets/img/icon-128.png",
      "16": "assets/img/icon-16.png",
      "32": "assets/img/icon-32.png",
      "48": "assets/img/icon-48.png"
   },
   "chrome_url_overrides": {
      "newtab": "newtab.html"
   },
   "permissions": [],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' https://ssl.google-analytics.com https://docs.google.com",
      "sandbox": "sandbox allow-scripts; script-src 'self' https://ssl.google-analytics.com https://docs.google.com; object-src 'self'"
   },
   "web_accessible_resources": []
}
